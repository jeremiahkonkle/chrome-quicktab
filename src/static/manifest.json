{
  "name": "QuickTab",
  "description" : "To use, type 't', then space, then a search term into the Omnibox.",
  "version": "1.0",
  "background": {
    "scripts": ["radtab-bundle.js"]
  },
  "omnibox": { "keyword" : "t" },
  "permissions": ["tabs", "webRequest", "webRequestBlocking", "<all_urls>"],
  "manifest_version": 2
}